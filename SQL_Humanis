select top(10000) distinct substring(TRIM(c.Siret),1,9) as siren_contrat,c.NumAdherent,c.NumAssure, c.NumAssure, s.xvar71 as siren, s.xnum26 as nb_assurables, s.xvar27 as segment_taille
from INFOCENTRE.TContratColl c
left join DECLIC.So0 s
on s.xvar71 = substring(TRIM(c.Siret,1,9))
where s.template is null
and c.DateEffet <= '2018-07-10' and c.DateRadiation > '2018-07-10' 
